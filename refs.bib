issn = {10959300},
keywords = {Animacy,Appearance,Lifeliness,Motion,Robots,Social robots},
pages = {27--38},
title = {{Effects of form and motion on judgments of social robots' animacy, likability, trustworthiness and unpleasantness}},
url = {http://www.sciencedirect.com/science/article/pii/S107158191600032X},
urldate = {2016-06-12},
volume = {90},
year = {2016}
}
