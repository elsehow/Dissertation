@article{Dourish2014,
abstract = {Design-oriented research is an act of collective imagining—a way in which we work together to bring about a future that lies slightly out of our grasp. In this paper, we examine the collective imagining of ubiquitous computing by bringing it into alignment with a related phenomenon, science fiction, in particular as imagined by a series of television shows that form part of the cultural backdrop for many members of the research community. A comparative reading of these fictional narratives highlights a series of themes that are also implicit in the research literature. We argue both that these themes are important considerations in the shaping of technological design and that an attention to the tropes of popular culture holds methodological value for ubiquitous computing.},
author = {Dourish, Paul and Bell, Genevieve},
