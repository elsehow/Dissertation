@article{Antoniou2017,
abstract = {Effective training of neural networks requires much data. In the low-data regime, parameters are underdetermined, and learnt networks generalise poorly. Data Augmentation $\backslash$cite{\{}krizhevsky2012imagenet{\}} alleviates this by using existing data more effectively. However standard data augmentation produces only limited plausible alternative data. Given there is potential to generate a much broader set of augmentations, we design and train a generative model to do data augmentation. The model, based on image conditional Generative Adversarial Networks, takes data from a source domain and learns to take any data item and generalise it to generate other within-class data items. As this generative process does not depend on the classes themselves, it can be applied to novel unseen classes of data. We show that a Data Augmentation Generative Adversarial Network (DAGAN) augments standard vanilla classifiers well. We also show a DAGAN can enhance few-shot learning systems such as Matching Networks. We demonstrate these approaches on Omniglot, on EMNIST having learnt the DAGAN on Omniglot, and VGG-Face data. In our experiments we can see over 13$\backslash${\%} increase in accuracy in the low-data regime experiments in Omniglot (from 69$\backslash${\%} to 82$\backslash${\%}), EMNIST (73.9$\backslash${\%} to 76$\backslash${\%}) and VGG-Face (4.5$\backslash${\%} to 12$\backslash${\%}); in Matching Networks for Omniglot we observe an increase of 0.5$\backslash${\%} (from 96.9$\backslash${\%} to 97.4$\backslash${\%}) and an increase of 1.8$\backslash${\%} in EMNIST (from 59.5$\backslash${\%} to 61.3$\backslash${\%}).},
archivePrefix = {arXiv},
arxivId = {1711.04340},
author = {Antoniou, Antreas and Storkey, Amos and Edwards, Harrison},
eprint = {1711.04340},
file = {:Users/ffff/Documents/Mendeley Desktop/1711.04340v1.pdf:pdf},
pages = {1--13},
title = {{Data Augmentation Generative Adversarial Networks}},
