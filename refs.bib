issn = {1662-5196},
journal = {Frontiers in neuroinformatics},
pages = {20},
pmid = {24124417},
shorttitle = {MOBBED},
title = {{MOBBED: a computational data infrastructure for handling large collections of event-rich time series datasets in MATLAB.}},
