journal = {Brain},
keywords = {abbreviations,coefficients,cortical anatomy,dz twins ϭ dizygotic,genetics,gyral patterns,human brain size,icc,manova ϭ,model,multiway analysis of variance,mz twins ϭ monozygotic,rms ϭ root-mean-square,same sex,se ϭ structural equation,twins,u,ϭ unbiased intraclass correlation},
