@article{Vidaurre2011c,
abstract = {All brain-computer interface (BCI) groups that have published results of studies involving a large number of users performing BCI control based on the voluntary modulation of sensorimotor rhythms (SMR) report that BCI control could not be achieved by a non-negligible number of subjects (estimated 20{\%} to 25{\%}). This failure of the BCI system to read the intention of the user is one of the greatest problems and challenges in BCI research. There are two main causes for this problem in SMR-based BCI systems: either no idle SMR is observed over motor areas of the user, or this idle rhythm is not modulated during motor imagery, resulting in a classification performance lower than 70{\%} (criterion level) that renders the control of a BCI application (like a speller) difficult or impossible. Previously, we introduced the concept of machine learning based co-adaptive calibration, which provided substantially improved performance for a variety of users. Here, we use a similar approach and investigate to what extent co-adaptive learning enables significant BCI control for completely novice users, as well as for those who could not achieve control with a conventional SMR-based BCI.},
annote = {replaces the offline calibration by a ‘co-adaptive calibration', in which the mental strategy of the user and the algorithm of the BCI system are jointly optimized. This approach leads completely novices users to achieve control of the system in just one session (and in the first minutes of operation). In addition, at least half of the category III users, who could not gain BCI control in the classic machine learning approach, could achieve control after 15 min of feedback (after the first run), thus reducing the percentage of users unable to communicate using SMR-based BCI systems from 25{\%} to 12.5{\%}. [..] 10 users out of 14 significantly improved their performance between the first and last run. 









 




 



----------







Users exhibit a different mental state during offline calibration and online feedback (cf [22]), which renders the classifier that is optimized on the data of the calibration suboptimal and sometimes even non-functional for feedback 




----------

From our results, at least half of the users who suffered from the inefficiency of the BCI systems can now perform BCI experiments with a good level of control, and this within just 15 min. The question remains whether operant conditioning or further sessions of co-adaptive calibration can provide BCI control to those users who continue without significant control after this study.},
author = {Vidaurre, Carmen and Sannelli, Claudia and M{\"{u}}ller, Klaus-Robert and Blankertz, Benjamin},
doi = {10.1088/1741-2560/8/2/025009},
file = {:Users/ffff/Documents/Mendeley Desktop/Vidaurre et al. - 2011 - Co-adaptive Calibration to improve BCI Efficiency.pdf:pdf},
isbn = {1741-2560},
issn = {1741-2560},
