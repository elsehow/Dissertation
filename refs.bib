@book{PeggyDeamereds2015,
abstract = {Directly confronting the nature of contemporary architectural work, this book is the first to address a void at the heart of architectural discourse and thinking. For too long, architects have avoided questioning how the central aspects of architectural "practice" (professionalism, profit, technology, design, craft, and building) combine to characterize the work performed in the architectural office. Nor has there been a deeper evaluation of the unspoken and historically-determined myths that assign cultural, symbolic, and economic value to architectural labor. The Architect as Worker presents a range of essays exploring the issues central to architectural labor. These include questions about the nature of design work; immaterial and creative labor and how it gets categorized, spatialized, and monetized within architecture; the connection between parametrics and BIM and labor; theories of architectural work; architectural design as a cultural and economic condition; entrepreneurialism; and the possibility of ethical and rewarding architectural practice. The book is a call-to-arms, and its ultimate goal is to change the practice of architecture. It will strike a chord with architects, who will recognize the struggle of their profession; with students trying to understand the connections between work, value, and creative pleasure; and with academics and cultural theorists seeking to understand what grounds the discipline.},
annote = {NULL},
author = {{Peggy Deamer eds}},
chapter = {12},
edition = {1st},
isbn = {9781472570529},
publisher = {Bloomsbury Academic},
title = {{The Architect As Worker: Immaterial Labor, the Creative Class, and the Politics of Design}},
year = {2015}
}
@article{Kunze2013,
abstract = {Applying mobile sensing technology to cognitive tasks will$\backslash$nenable novel forms of activity recognition. Physical activity$\backslash$nrecognition technology has become mainstream-many dedicated mobile$\backslash$ndevices and smartphone apps count the steps we climb or the miles we$\backslash$nrun. What if devices and apps were also available that could count the$\backslash$nwords we read and how far we've progressed in our learning? The authors$\backslash$nof this article demonstrate that mobile eye tracking can be used to do$\backslash$njust that. Focusing on reading habits, they've prototyped cognitive$\backslash$nactivity recognition systems that monitor what and how much users read$\backslash$nas well as how much they understand. Such systems could revolutionize$\backslash$nteaching, learning, and assessment both inside and outside the$\backslash$nclassroom. Further, as sensing technology improves, activity recognition$\backslash$ncould be extended to other cognitive tasks including concentrating,$\backslash$nretaining information, and auditory or visual processing. While this$\backslash$nresearch is extremely exciting, it also raises numerous ethical$\backslash$nquestions-for example, who should know what we read or how much we$\backslash$nunderstand?},
author = {Kunze, Kai and Iwamura, Masakazu and Kise, Koichi and Uchida, Seiichi and Omachi, Shinichiro},
