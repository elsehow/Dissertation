isbn = {1756050031},
issn = {1662-5196},
journal = {Frontiers in neuroinformatics},
keywords = {electrophysiology,event related potentials,infrastructure,neuroinformatics,portal,processing methods,signal,stimulator,workflow},
month = {mar},
number = {March},
