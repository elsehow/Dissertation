@inproceedings{Merrill2017,
abstract = {This study examines heartrate sharing in the context of a trust-building game. Through quantitative and qualitative analysis, we find that "elevated" (versus “normal”) heartrate causes participants to make negative mood attributions about their partner, and to cooperate less frequently. To investigate how specific our findings are to heartrate (as opposed to some other "elevated" signal collected from the body), we replicate our initial experiment with a fictional biosignal. We find that both heartrate and the fictional signal are associated with negative mood attributions, but we observe a decrease in cooperative behavior only with elevated heartrate. Qualitative results indicate that individuals may learn an association between our fictional biosignal and the cooperative, trusting behavior of their partner. Our findings highlight the role prior beliefs can play in bounding possible interpretations of a biosignal, while suggesting that designers can, perhaps inadvertently, train users to associate ambiguous signals with arbitrary social meanings. We discuss implications for technology- mediated social interactions with wearable sensors.},
address = {Portland, OR},
author = {Merrill, Nick and Cheshire, Coye},
booktitle = {Proceedings of the 2017 ACM Conference on Computer Supported Cooperative Work (CSCW '17)},
keywords = {Computer-mediated communication,biosignals,cooperation,heartrate,sensing,trust},
