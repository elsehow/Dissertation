title = {{Single trial analysis of slow cortical potentials: a study on anticipation related potentials}},
url = {http://infoscience.epfl.ch/record/185106/files/Garipelli{\_}JNE{\_}2013.pdf{\%}5Cnhttp://www.ncbi.nlm.nih.gov/pubmed/23611808},
