issn = {03029743},
journal = {UbiComp 2006: Ubiquitous Computing},
keywords = {Computer Communication Networks,Computers and Society,Folder - fall 2014 - contextual sensing,Information Systems Applications (incl.Internet),Operating Systems,Software Engineering,User Interfaces and Human Computer Interaction,sensing},
mendeley-tags = {Folder - fall 2014 - contextual sensing,sensing},
pages = {261--278},
publisher = {Springer Berlin Heidelberg},
shorttitle = {Fish'n'Steps},
title = {{Fish'n'Steps: Encouraging Physical Activity with an Interactive Computer Game}},
url = {http://link.springer.com.libproxy.usc.edu/chapter/10.1007/11853565{\_}16{\%}5Cnfiles/3571/Lin et al. - 2006 - Fish?n?Steps Encouraging Physical Activity with a.pdf{\%}5Cnfiles/3588/10.html},
