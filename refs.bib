@article{Soucek2010,
abstract = {The present paper introduces three facets of information overload in email communication: A large amount of incoming information, inefficient workflow, and deficient communication quality. In order to cope with these facets of information overload, a training intervention was developed and evaluated. Data were collected from 90 employees on several evaluation levels within a longitudinal evaluation design (one pretest double posttest design). The results reveal that the training contributed to an increase in knowledge and media competencies. We also found evidence for a transfer of training contents to the workplace. Finally, strain diminished on several dimensions. In particular, problems with media usage and work impairment decline significantly, an effect that was stronger for those participants who face a large amount of email at their workplaces. ?? 2010 Elsevier Ltd. All rights reserved.},
annote = {
