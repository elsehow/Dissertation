journal = {Journal of Agricultural and Food Chemistry},
keywords = {Cytotoxicity,Flavokavain,Kava,Kava toxicity,Piper methysticum},
number = {8},
pages = {3157--3162},
pmid = {16608246},
title = {{In vitro cytotoxicity of nonpolar constituents from different parts of kava plant (Piper methysticum)}},
volume = {54},
year = {2006}
}
