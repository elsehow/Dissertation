isbn = {0018-9219},
issn = {00189219},
journal = {Proceedings of the IEEE},
keywords = {Augmented brain-computer interface (ABCI),Folder - bmi - exploratory,brain-computer interaction,electroencephalographic (EEG),human-computer interaction,opportunistic BCI,opportunistic state detection,pervasive computing},
