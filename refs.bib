issn = {15344320},
journal = {IEEE Transactions on Neural Systems and Rehabilitation Engineering},
keywords = {Closed-loop feedback,Folder - bmi - exploratory - memory prosthesis,cortical neural prosthesis,delayed memory task,hippocampal ensemble activity,neural stimulation,nonlinear mathematical model,performance enhancement},
mendeley-tags = {Folder - bmi - exploratory - memory prosthesis},
month = {mar},
number = {2},
pages = {184--197},
pmid = {22438334},
shorttitle = {A Nonlinear Model for Hippocampal Cognitive Prosth},
title = {{A nonlinear model for hippocampal cognitive prosthesis: Memory facilitation by hippocampal ensemble stimulation}},
