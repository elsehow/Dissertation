@article{parra_recipes_2005,
	title = {Recipes for the linear analysis of {EEG}},
	volume = {28},
	abstract = {In this paper, we describe a simple set of "recipes" for the analysis of high spatial density EEG. We focus on a linear integration of multiple channels for extracting individual components without making any spatial or anatomical modeling assumptions, instead requiring particular statistical properties such as maximum difference, maximum power, or statistical independence. We demonstrate how corresponding algorithms, for example, linear discriminant analysis, principal component analysis and independent component analysis, can be used to remove eye-motion artifacts, extract strong evoked responses, and decompose temporally overlapping components. The general approach is shown to be consistent with the underlying physics of EEG, which specifies a linear mixing model of the underlying neural and nonneural current sources},
	journal = {NEUROIMAGE},
	author = {Parra, Lucas C. and Spence, Clay D. and Gerson, Adam D. and Sajda, Paul},
	year = {2005},
	pages = {326--341},
	file = {Citeseer - Full Text PDF:/Users/ffff/Library/Application Support/Zotero/Profiles/7dlfw4z1.default/zotero/storage/VVJVH7I7/Parra et al. - 2005 - Recipes for the linear analysis of EEG.pdf:application/pdf;Citeseer - Snapshot:/Users/ffff/Library/Application Support/Zotero/Profiles/7dlfw4z1.default/zotero/storage/G4WCXZBZ/Parra et al. - 2005 - Recipes for the linear analysis of EEG.html:text/html}
