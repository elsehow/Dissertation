	month = oct,
	year = {2013},
	keywords = {Application control, Brain–computer interface (BCI), electroencephalogram (EEG), End-user, motor imagery, Technology transfer},
	pages = {121--132},
	file = {ScienceDirect Full Text PDF:/Users/ffff/Library/Application Support/Zotero/Profiles/7dlfw4z1.default/zotero/storage/4RDTZ5SV/Leeb et al. - 2013 - Transferring brain–computer interfaces beyond the .pdf:application/pdf;ScienceDirect Snapshot:/Users/ffff/Library/Application Support/Zotero/Profiles/7dlfw4z1.default/zotero/storage/HVUHV3QJ/S0933365713001218.html:text/html}
