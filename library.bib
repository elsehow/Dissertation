	pages = {1--16},
	file = {Full Text PDF:/Users/ffff/Library/Application Support/Zotero/Profiles/7dlfw4z1.default/zotero/storage/DFT8EKHF/Chuang et al. - 2013 - I Think, Therefore I Am Usability and Security of.pdf:application/pdf;Snapshot:/Users/ffff/Library/Application Support/Zotero/Profiles/7dlfw4z1.default/zotero/storage/G5R3UXZJ/978-3-642-41320-9_1.html:text/html}
